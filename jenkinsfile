@Library('mylib')_
pipeline
{
    agent any
    stages
    {
        stage('down')
        {
            steps
            {
               script
               {
                   cicd3.newGit("maven")
               }
            }
        }
        stage('build')
        {
            steps
            {
                script
                {
                    cicd3.newMaven()
                }
            }
        }
    }	
}
